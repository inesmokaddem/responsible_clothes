<div class="reviews_product">
  <span class="stars_rating"><% review.stars.times do %>
    <%= inline_svg("star.svg", width: 10, height: 20, class: "collapse-icon") %>
  <% end %>
  <% (5 - review.stars).times do %>
    <%= inline_svg("empty_star.svg", width: 10, height: 20, class: "collapse-icon") %>
  <% end %>
  </span>
  <p class="comment"><%= review.content %></p>
</div>
